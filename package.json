{
  "name": "app-react-features",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier:options": "prettier --ignore-unknown src/**",
    "lint:eslint": "eslint src/**/*.{js,ts,jsx,tsx}",
    "lint:prettier": "npm run prettier:options -- --check",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run prettier:options -- --write"
  },
  "lint-staged": {
    "src/**": "prettier --write --ignore-unknown",
    "src/**/*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "clsx": "^1.2.1",
    "nanoid": "^4.0.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.7",
    "sass": "^1.60.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
