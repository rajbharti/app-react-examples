{
  "name": "app-react-features",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier:options": "prettier --ignore-unknown src/**",
    "lint:eslint": "eslint src/**/*.{js,ts,jsx,tsx}",
    "lint:prettier": "npm run prettier:options -- --check",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run prettier:options -- --write"
  },
  "lint-staged": {
    "src/**": "prettier --write --ignore-unknown",
    "src/**/*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@picocss/pico": "^1.5.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
